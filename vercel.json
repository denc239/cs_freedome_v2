{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ],
  "env": {
    "NEXT_PUBLIC_APP_NAME": "CS_Freedome v2",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "regions": ["fra1"],
  "github": {
    "enabled": true
  }
}